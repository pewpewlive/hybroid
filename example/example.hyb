struct Cube {
  x, y = 20.5f, 30.01f
  size = 5f

  name = "cube :3"

  health = 3

  new(x fixed, y fixed) {
    self.x = x
    self.y = y
  }

/*
 fun func
*/
  fn SetPos(newX fixed, newY fixed) {
    self.x = newX
    self.y = newY
  } 

  fn Move(dirX fixed, dirY fixed) {
    self.x += dirX
    self.y += dirY
  }
}

let cube = new Cube(10f, 10f)

repeat 10 with time {
  cube.SetPos(cube.x + 1f, time)
  
  if cube.y < 0f {
    cube.Move(0f, 10f)

    continue 
  }
} 